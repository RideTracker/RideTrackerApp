{
  "expo": {
    "name": "Ride Tracker",
    "slug": "ride-tracker",
    "owner": "nora-soderlund",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#16171F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "The Ride Tracker will use your location to record your location when you're recording a ride",
        "UIBackgroundModes": [
          "location"
        ]
      },
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.norasoderlund.ridetracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "package": "com.norasoderlund.ridetracker",
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload a profile picture and activity images."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7aa00640-909f-434c-8b6e-524b358adc78"
      }
    }
  }
}
